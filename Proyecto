                                xnum = Integer.parseInt(JOptionPane.showInputDialog("Ingrese número del alumno a suspender."));
                                for (x = 1; x <= 30; x++) {
                                    if ((xnum == numa[x]) && (acta[x] != 0)) {
                                        acta[x] = 0;
                                        exi = 1;
                                    }
                                }
                                if (exi == 1) {
                                    JOptionPane.showMessageDialog(null, "Suspención establecida.", "SUSPENCION", JOptionPane.INFORMATION_MESSAGE);
                                } else {
                                    JOptionPane.showMessageDialog(null, "No se encuentra el alumno.", "ERROR", JOptionPane.INFORMATION_MESSAGE);
                                }
                            } else if (subm == 3) {
                                xnum = Integer.parseInt(JOptionPane.showInputDialog("Ingrese número del alumno a reinstalar."));
                                for (x = 1; x <= 30; x++) {
                                    if ((xnum == numa[x]) && (acta[x] != 0)) {
                                        acta[x] = 0;
                                        exi = 1;
                                    }
                                }
                                if (exi == 1) {
                                    JOptionPane.showMessageDialog(null, "Reinstalaión establecida.", "REINSTALACION", JOptionPane.PLAIN_MESSAGE);
                                } else {
                                    JOptionPane.showMessageDialog(null, "No se encuentra el alumno.", "ERROR", JOptionPane.ERROR_MESSAGE);
                                }
                            } else if (subm == 4) {
                            } else {
                                JOptionPane.showMessageDialog(null, "Ación o encontrada.", "ERROR", JOptionPane.ERROR_MESSAGE);
                            }
                        } while (subm != 4);
                    }else{
                        JOptionPane.showMessageDialog(null, "Ingrese un registro", "Alumno", JOptionPane.PLAIN_MESSAGE);
                    }
                } else if (menu == 4) {
                    if(bully==1){
                        xnum = Integer.parseInt(JOptionPane.showInputDialog("¿De que alumno va a cambiar calificaciones?\n\n Ingrese su número"));
                        for (x = 1; x <= 30; x++) {
                            if ((xnum == numa[x]) && (acta[x] != 0)) {

                            }
                        }
                    }else{
                        JOptionPane.showMessageDialog(null, "ingrese un registro" +
                                "", "Alumno", JOptionPane.PLAIN_MESSAGE);
                    }
                } else if (menu == 5) {
                    JOptionPane.showMessageDialog(null, "TENGA UN BUEN DIA, ADIOS", "Adios", JOptionPane.INFORMATION_MESSAGE);
                    armando.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
                    System.exit(0);
                }
            }catch (Exception FormatException) {
                JOptionPane.showMessageDialog(null, "por favor ingrese algo valido", "ERROR", JOptionPane.ERROR_MESSAGE);
            }
        }while (menu != 5);
    }
}
